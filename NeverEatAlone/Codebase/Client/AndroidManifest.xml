<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.jhu.cs.oose.fall2014.group19.neverEatAlone.client"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.jhu.cs.oose.fall2014.group19.neverEatAlone.client.gui.activities.MainActivity"
           android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.jhu.cs.oose.fall2014.group19.neverEatAlone.client.gui.activities.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="edu.jhu.cs.oose.fall2014.group19.neverEatAlone.client.gui.activities.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.jhu.cs.oose.fall2014.group19.neverEatAlone.client.gui.activities.MainActivity" />
        </activity>
        
        <activity
            android:name="edu.jhu.cs.oose.fall2014.group19.neverEatAlone.client.gui.activities.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
    </application>

</manifest>
